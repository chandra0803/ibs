<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:flx="http://www.fuelplus.de/flxXML/v2.0" elementFormDefault="unqualified"
	attributeFormDefault="unqualified" version="2.0">

	<xs:annotation>
		<xs:documentation>
			Schema Name: flx
			Copyright: Fuelplus
			Schema Version
			Number: 2.0 Version
			Date: 2010-03-04
			Notes:
			Schema Desc:
		</xs:documentation>
	</xs:annotation>
	<!-- 17/03/2010 mf Additions aus der eMail vom Christian Mietz 17/03/10
		minOccurs="1" maxOccurs="1" wurde nicht added da Standardeinsellung Unit
		use wurde pauschal auf required gesetzt da optional Standard -->
	<xs:simpleType name="EmptyString">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse" />
			<xs:enumeration value="" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ACType">
		<xs:annotation>
			<xs:documentation>defines the type of an AC: Length 1-3 chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SRCType">
		<xs:annotation>
			<xs:documentation>defines the type of an source: Length 1-3
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlTypeType">
		<xs:annotation>
			<xs:documentation>defines the type of an FL Type: Length 1-5
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="5" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceTypeType">
		<xs:annotation>
			<xs:documentation>defines the type of an ServiceType: Length
				1-10
				chars.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DivType">
		<xs:annotation>
			<xs:documentation>defines the type of an division: Length 1-6
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="6" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegistrationType">
		<xs:annotation>
			<xs:documentation>defines the type of an registration: Length
				1-10
				chars.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ICAOType">
		<xs:annotation>
			<xs:documentation>defines the type of an ICAO: Length 1-32
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="32" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MissionIdType">
		<xs:annotation>
			<xs:documentation>defines the type of an Mission Id: Length
				1-25
				chars.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="25" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeType">
		<xs:annotation>
			<xs:documentation>defines the type of an Type: Length 1-26
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="26" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OFPIDType">
		<xs:annotation>
			<xs:documentation>OPF number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"></xs:minLength>
			<xs:maxLength value="10"></xs:maxLength>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OIType">
		<xs:annotation>
			<xs:documentation>defines the type of an OI: Length 1-512 chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"></xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CompanyType">
		<xs:annotation>
			<xs:documentation>defines the type of an company: Length 1-3
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CompanyTypeExt">
		<xs:annotation>
			<xs:documentation>defines the type of an company: Length 1-25
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="25" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DomIntType">
		<xs:annotation>
			<xs:documentation>defines the type of an Dom/Int Flag: Length
				1 char,
				D or
				I.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D" />
			<xs:enumeration value="I" />
			<xs:enumeration value="U" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SuffixType">
		<xs:annotation>
			<xs:documentation>defines the type of an Flight Suffix: Length
				1 chars
				Uppercase
				letter. Part of the Primary Key must have length 1
				in Oracle
				DB</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="1" />
			<xs:pattern value="[A-Z\s]" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightNumberType">
		<xs:annotation>
			<xs:documentation>defines the type of an FlightNumber: Length
				1-4
				digits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="4" />
			<xs:pattern value="([0-9])+" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationType">
		<xs:annotation>
			<xs:documentation>defines the type of an Location: Length 1-4
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="4" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UnitType">
		<xs:annotation>
			<xs:documentation>defines the type of a unit: Length 1-3
				chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Unit2Type">
		<xs:restriction base="xs:string">
			<xs:pattern value="[\w]{1,3}/[\w]{1,3}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Unit2OptType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[\w]{1,3}(/[\w]{1,3})?"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="SeqNrType">
		<xs:annotation>
			<xs:documentation>defines the type of an sequence number:
				Length 1-10
				digits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateType">
		<xs:annotation>
			<xs:documentation>defines the type of an Date. Format:
				"YYYY-MM-DD"
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date" />
	</xs:simpleType>
	<xs:simpleType name="DateTimeType">
		<xs:annotation>
			<xs:documentation>defines the type of an DateTime. Format:
				"YYYY-MM-DDThh:mm:ss"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime" />
	</xs:simpleType>
	<xs:simpleType name="CountType">
		<xs:annotation>
			<xs:documentation>defines the type of an Count.
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger" />
	</xs:simpleType>
	<xs:simpleType name="WeightType">
		<xs:annotation>
			<xs:documentation>defines the type of an Weight, total digits
				15.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="15" />
			<xs:fractionDigits value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DistanceType">
		<xs:annotation>
			<xs:documentation>defines the type of an Distance, total
				digits 12.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="12" />
			<xs:fractionDigits value="3" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DensityValueType">
		<xs:annotation>
			<xs:documentation>defines the type of a density value, total
				digits
				13.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="13" />
			<xs:fractionDigits value="4" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="QuantityType">
		<xs:annotation>
			<xs:documentation>defines the type of an Quantity, total
				digits 13.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="13" />
			<xs:fractionDigits value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IndexType">
		<xs:annotation>
			<xs:documentation>defines the type of an Load Index
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal" />
	</xs:simpleType>
	<xs:simpleType name="MACType">
		<xs:restriction base="xs:decimal" />
	</xs:simpleType>
	<xs:simpleType name="StabilizerType">
		<xs:restriction base="xs:decimal" />
	</xs:simpleType>
	<xs:simpleType name="RemarkType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RouteCodeType">
		<xs:annotation>
			<xs:documentation>defines the type of OFP Route Code
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="50" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DurationType">
		<xs:annotation>
			<xs:documentation>
				defines the type of Flight Time 0000 (hhmm),
				without
				leading zero
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="5" />
			<xs:maxLength value="6" />
			<xs:pattern value="-?([01]?[0-9]|2[0-3]):([0-5][0-9])"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TReasonType">
		<xs:annotation>
			<xs:documentation>defines the type of Tankering Reason
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"></xs:maxLength>
			<xs:minLength value="1"></xs:minLength>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TRecommendType">
		<xs:annotation>
			<xs:documentation>defines the type of Tankering Fuel
				Recommended
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1" />
			<xs:enumeration value="Y" />
			<xs:enumeration value="N" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AltitudeType">
		<xs:annotation>
			<xs:documentation>defines the type of Altitudes
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer" />
	</xs:simpleType>
	<xs:simpleType name="ProfitType">
		<xs:annotation>
			<xs:documentation>defines the type of Profit
      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer" />
	</xs:simpleType>
	<xs:simpleType name="CostIndexType">
		<xs:restriction base="xs:integer" />
	</xs:simpleType>
	<xs:simpleType name="AirwayType">
		<xs:annotation>
			<xs:documentation>defines the airway: Length 1-10 chars.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>

	<!-- ************************************** End of Types ********************* -->

	<xs:element name="flx" type="flxType" />
	<xs:complexType name="flxType">
		<xs:sequence>
			<xs:element ref="Flight" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="Flight" type="FlightType" />
	<xs:complexType name="FlightType">
		<xs:annotation>
			<xs:documentation> defines the data for one flight.
			</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element ref="FId" maxOccurs="1" minOccurs="1" />
			<xs:element ref="AcMvmt" minOccurs="0" />
			<xs:element ref="ATC" minOccurs="0" />
			<xs:element ref="Fuel" minOccurs="0" />
			<xs:element ref="FlightFuelLog" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element ref="AlSpec" minOccurs="0" />
			<xs:element ref="Load" minOccurs="0" />
			<xs:element ref="OFP" minOccurs="0" />
			<xs:element ref="ACARS" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element ref="FDM" maxOccurs="1" minOccurs="0"></xs:element>
			<xs:element ref="OpFuel" maxOccurs="1" minOccurs="0"></xs:element>
		</xs:all>
		<xs:attribute name="mode" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="flight" />
					<xs:enumeration value="data" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:element name="FId" type="FIdType" />
	<xs:complexType name="FIdType">
		<xs:annotation>
			<xs:documentation> All data in order to identify a flight. Dte
				FlightDate Nr
				FlightNumber Sf Suffix AlD Airline Designator Org
				Origin Dst Destination Reg
				Registration (required if mode = "flight")
				Own AircraftOwner (required if mode =
				"flight") Op FlightOperator
				(required if mode = "flight") AC OperationalAircraftType
				(required if
				mode = "flight") DI DomInt (required if mode = "flight")
			</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Dte" type="DateType" />
			<xs:element name="Nr" type="FlightNumberType" />
			<xs:element name="Sf" type="SuffixType" minOccurs="0" />
			<xs:element name="AlD" type="CompanyType" />
			<xs:element name="Org" type="LocationType" />
			<xs:element name="Dst" type="LocationType" />
			<xs:element name="SchDst" type="LocationType" maxOccurs="1"
				minOccurs="0"></xs:element>
			<xs:element name="Reg" type="RegistrationType" />
			<xs:element name="Own" type="CompanyType" minOccurs="0" />
			<xs:element name="Op" type="CompanyType" minOccurs="0" />
			<xs:element name="AC" type="ACType" minOccurs="0" />
			<xs:element name="DI" type="DomIntType" minOccurs="0" />
			<xs:element name="FltFlg" minOccurs="0" maxOccurs="1"
				type="FltFlgTypeNullable"></xs:element>
		</xs:all>
	</xs:complexType>

	<xs:element name="AcMvmt" type="AcMvmtType" />
	<xs:complexType name="AcMvmtType">
		<xs:annotation>
			<xs:documentation> Aircraft Movement Data / Timestamps (required if
				mode = "flight")
				OfBl Off Block (UTC) TkOf Take Off (UTC) TDwn Touch
				Down (UTC) OnBl On Block (UTC)
				OfBlLt Off Block in Local Time
			</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="OfBl" type="DateTimeType" minOccurs="0" />
			<xs:element name="TkOf" type="DateTimeType" minOccurs="0" />
			<xs:element name="TDwn" type="DateTimeType" minOccurs="0" />
			<xs:element name="OnBl" type="DateTimeType" minOccurs="0" />
			<xs:element name="OfBlLt" type="DateTimeType" minOccurs="0" />
		</xs:all>
	</xs:complexType>

	<xs:element name="ATC" type="ATCType" />
	<xs:complexType name="ATCType">
		<xs:annotation>
			<xs:documentation> Air Traffic Control Data. ICAO Aircraft
				Identification (ICAO Call
				Sign, Field 7) ICAO call sign, any length
				Type TypeOfFlight (EU ETS Applicable
				Indicator, Field 8) according to
				CRCO codes of EUROCONTROL OI Other Information
				(Field 18) according
				to excemption rules of EU ETS If no values are available the whole
				section will not transmitted</xs:documentation>
		</xs:annotation>

		<xs:all>
			<xs:element name="ICAO" type="ICAOType" />
			<xs:element name="Type" type="TypeType" minOccurs="0" />
			<xs:element name="Route" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"></xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OI" type="OIType" minOccurs="0" />
		</xs:all>
	</xs:complexType>

	<xs:element name="Fuel" type="FuelType" />
	<xs:complexType name="FuelType">
		<xs:annotation>
			<xs:documentation> Src source of the fuel information (FMM or STD or
				DLK, etc) DefU used
				for all units if not given. Not used for density!
				(KG or LT or UG) Default is
				Sysweight. Sdp ShutdownFuel of preflight
				SdpU ShutdownFuelUnit of preflight (default
				= DefU) Rem RemainingFuel
				RemU RemainingFuelUnit (default = DefU) Up UpliftFuel UpU
				UpliftFuelUnit (default = DefU) Dns UpliftDensity (Sysdensity used
				if not given)
				DnsMasU UpliftDensityMassUnit (Sysweight used if not
				given) DnsVolU
				UpliftDensityVolumeUnit (Sysvol used if not given) Sup
				Supplier Of OffblockFuel OfU
				OffblockFuelUnit (default = DefU) Dmp
				Fuel Dump DmpU Fuel dump Unit Sd ShutdownFuel
				SdU ShutdownFuelUnit
				(default = DefU) BC BlockConsumption BCU BlockConsumptionUnit
				(default = DefU) TC TotalConsumption TCU TotalConsumptionUnit
				(default = DefU)
      </xs:documentation>
		</xs:annotation>

		<xs:all>
			<xs:element name="Src" type="SRCType" />
			<xs:element name="DefU" type="UnitType" minOccurs="0" />
			<xs:element name="Sdp" minOccurs="0" type="SdpType">
			</xs:element>
			<xs:element name="Rem" minOccurs="0" type="RemType">
			</xs:element>
			<xs:element name="Up" minOccurs="0" type="UpType">
			</xs:element>
			<xs:element name="Dns" minOccurs="0" type="DnsType">
			</xs:element>
			<xs:element name="Sup" type="CompanyType" minOccurs="0" />
			<xs:element name="Of" minOccurs="0" type="OfType">
			</xs:element>
			<xs:element name="Dmp" minOccurs="0" type="DmpType">
			</xs:element>
			<xs:element name="Sd" minOccurs="0" type="SdType">
			</xs:element>
			<xs:element name="BC" minOccurs="0" type="BCType">
			</xs:element>
			<xs:element name="TC" minOccurs="0" type="TCType">
			</xs:element>
		</xs:all>
	</xs:complexType>

	<xs:element name="AlSpec" type="AlSpecType" />
	<xs:complexType name="AlSpecType">
		<xs:annotation>
			<xs:documentation> Airline Specifics additional airline specific
				data, formerly loaded
				via SE06 Div Division DLH, division marker
				RskHld RiskHolder DLH, Company
				responsible for the fuel bill RepCus
				ReportedCustomer AAWH SeqNr
				FlightSequenceNumber AAWH FlTyp
				FlightType AAWH STyp ServiceType AAWH Dist
				DistanceFlown AAWH DistU
				DistanceFlownUnit AAWH MisId MissionId AAWH
      </xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Div" type="DivType" minOccurs="0" />
			<xs:element name="RskHld" type="CompanyType" minOccurs="0" />
			<xs:element name="RepCus" type="CompanyType" minOccurs="0" />
			<xs:element name="SeqNr" type="SeqNrType" minOccurs="0" />
			<xs:element name="FlTyp" type="FlTypeType" minOccurs="0" />
			<xs:element name="STyp" type="ServiceTypeType" minOccurs="0" />
			<xs:element name="Dist" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="DistanceType">
							<xs:attribute name="u" type="UnitType" use="optional" />
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="MisId" type="MissionIdType" minOccurs="0" />
			<xs:element name="RCode" type="RouteCodeType" maxOccurs="1"
				minOccurs="0"></xs:element>
