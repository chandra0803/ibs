<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/PAUSRR_16_1_1A" xmlns="http://xml.amadeus.com/PAUSRR_16_1_1A" elementFormDefault="qualified">
  <xs:element name="PNR_SearchReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Return a list of PNR(s) matching search criteria</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorHandler" minOccurs="0" type="ErrorGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Filled when an error was encountered on server side and no output could be provided</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="scrollingDetails" minOccurs="0" type="ActionDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">This segment is used when search result must be split in several messages (follow up).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfOffices" minOccurs="0" type="NumberOfUnitsType" />
        <xs:element name="pnrViews" minOccurs="0" maxOccurs="300">
          <xs:annotation>
            <xs:documentation xml:lang="en">View of PNR(s) matching the search criteria.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="pnrNumber" type="ItemNumberTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Identifies the number of the PNR and its type 1 for the 1st PNR image 2 for the 2nd PNR image ...</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pnrView" type="StructuredBookingRecordImageType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">View of one PNR matching the search criteria.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the action that should be taken on a selected reference number.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="lastItemsDetails" type="ReferenceTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">This composite conveys the details needed to perfom the scrolling mechanism : indication of remaining items and last item retrieved.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
